{
  "name": "bulk-match-provider",
  "version": "1.0.0",
  "description": "Bulk Match Provider",
  "source": "./app/index.html",
  "scripts": {
    "start": "npm run clean && parcel build ./app/index.html && ts-node --transpile-only ./src/index.ts",
    "start:watch": "nodemon -e ts -x 'npm run start'",
    "test": "DOTENV_CONFIG_PATH=.env.test nyc --reporter=lcov mocha -r ts-node/register",
    "test:watch": "nodemon -w ./test -w ./src -e ts,json -x 'npm test'",
    "start:app": "parcel",
    "clean": "rm -rf ./dist/*"
  },
  "targets": {
    "default": {
      "source": "./app/index.html",
      "distDir": "./dist"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smart-on-fhir/bulk-match-provider.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smart-on-fhir/bulk-match-provider/issues"
  },
  "homepage": "https://github.com/smart-on-fhir/bulk-match-provider#readme",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "moment": "^2.30.1",
    "proper-lockfile": "^4.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fhir": "^0.0.41",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.26",
    "@types/proper-lockfile": "^4.1.4",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "constants-browserify": "^1.0.0",
    "events": "^3.3.0",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "nyc": "^15.1.0",
    "parcel": "^2.12.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  }
